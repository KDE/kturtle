#: interpreter/executer.cpp:395
#, kde-format
msgid "The %1 command accepts no parameters."
msgstr "%1 आदेशः कोऽपि पैरामीटर् न स्वीकुर्वति ।"

#: interpreter/executer.cpp:405
#, kde-format
msgid "The %2 command was called with %3 but needs 1 parameter."
msgid_plural "The %2 command was called with %3 but needs %1 parameters."
msgstr[0] "%2 आदेशः %3 इत्यनेन सह आहूतः परन्तु 1 पैरामीटर् आवश्यकम् ।"
msgstr[1] "%2 आदेशः %3 इत्यनेन सह आहूतः परन्तु %1 पैरामीटर् आवश्यकम् ।"

#: interpreter/executer.cpp:413
#, kde-format
msgid "The %2 command was called with %3 but only accepts 1 parameter."
msgid_plural ""
"The %2 command was called with %3 but only accepts %1 parameters."
msgstr[0] "%2 आदेशः %3 इत्यनेन सह आहूतः परन्तु केवलं 1 पैरामीटर् स्वीकुर्वति ।"
msgstr[1] "%2 आदेशः %3 इत्यनेन सह आहूतः परन्तु केवलं %1 मापदण्डान् स्वीकुर्वति ।"

#: interpreter/executer.cpp:436
#, kde-format
msgid "The %1 command only accepts a string as its parameter."
msgstr "%1 आदेशः केवलं स्ट्रिंग् स्वस्य पैरामीटर् रूपेण स्वीकुर्वति ।"

#: interpreter/executer.cpp:438
#, kde-format
msgid "The %1 command only accepts strings as its parameters."
msgstr "%1 आदेशः केवलं स्ट्रिंग् इत्येतत् स्वस्य पैरामीटर् रूपेण स्वीकुर्वति ।"

#: interpreter/executer.cpp:443
#, kde-format
msgid "The %1 command only accepts a number as its parameter."
msgstr "%1 आदेशः केवलं सङ्ख्यां स्वस्य पैरामीटर् रूपेण स्वीकुर्वति ।"

#: interpreter/executer.cpp:445
#, kde-format
msgid "The %1 command only accepts numbers as its parameters."
msgstr "%1 आदेशः केवलं सङ्ख्याः स्वस्य मापदण्डरूपेण स्वीकुर्वति ।"

#: interpreter/executer.cpp:450
#, kde-format
msgid "The %1 command only accepts an answer as its parameter."
msgstr "%1 आदेशः केवलं उत्तरं स्वस्य पैरामीटर् रूपेण स्वीकुर्वति ।"

#: interpreter/executer.cpp:452
#, kde-format
msgid "The %1 command only accepts answers as its parameters."
msgstr "%1 आदेशः केवलं उत्तराणि स्वस्य मापदण्डरूपेण स्वीकुर्वति ।"

#: interpreter/executer.cpp:526
#, kde-format
msgid "The variable '%1' was used without first being assigned to a value"
msgstr "प्रथमं मूल्यं न नियुक्तं कृत्वा '%1' इति चरं प्रयुक्तम्"

#: interpreter/executer.cpp:546
#, kde-format
msgid "An unknown function named '%1' was called"
msgstr "'%1' इति नाम्ना अज्ञातं फंक्शन् आहूतम्"

#: interpreter/executer.cpp:560
#, kde-format
msgid "The function '%1' was called with %2, while it should be called with %3"
msgstr "'%1' इति फंक्शन् %2 इत्यनेन सह आहूतम्, यदा तु %3 इत्यनेन आह्वानं कर्तव्यम्"

#: interpreter/executer.cpp:562 interpreter/executer.cpp:564
#, kde-format
msgctxt ""
"The function '%1' was called with %2, while it should be called with %3"
msgid "1 parameter"
msgid_plural "%1 parameters"
